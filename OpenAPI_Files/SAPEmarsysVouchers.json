{
    "openapi": "3.0.3",
    "info": {
        "title": "SAP Emarsys Vouchers",
        "description": "This API for allows you to add new voucher codes, delete old ones, reset the voucher pools and show the details of the pool.",
        "version": "1.0.0"
    },
    "x-sap-shortText": "The SAP Emarsys API that helps you manage your voucher pools. It allows you to add new codes, delete old ones, reset the pools as well as learn the voucher pool details.",
    "x-sap-compliance-level": "sap:core:v1",
    "externalDocs": {
        "description": "For more information, please visit our official SAP Emarsys developer portal.",
        "url": "https://dev.emarsys.com/docs/vouchers-api-reference"
    },
    "servers": [
        {
            "url": "https://{host}/api/coupon/v2",
            "description": "This is the production URL.",
            "variables": {
                "host": {
                    "default": "api.emarsys.net"
                }
            }
        }
    ],
    "paths": {
        "/couponPools/{couponPoolId}": {
            "get": {
                "description": "Shows the details of a pool.",
                "summary": "Show pool",
                "tags": [
                    "Voucher Pool"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/CouponPoolId"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Details of the pool",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/CouponPool"
                                        },
                                        {
                                            "$ref": "#/components/schemas/AllowNewCodeToSameContact"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "500": {
                        "$ref": "#/components/responses/ServerError"
                    }
                }
            },
            "patch": {
                "description": "Updates properties of an existing pool.",
                "summary": "Update pool",
                "tags": [
                    "Voucher Pool"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/CouponPoolId"
                    }
                ],
                "requestBody": {
                    "$ref": "#/components/requestBodies/CouponPoolForUpdate"
                },
                "responses": {
                    "200": {
                        "description": "Details of the pool (with the updated properties)",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/CouponPool"
                                        },
                                        {
                                            "$ref": "#/components/schemas/AllowNewCodeToSameContact"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "400": {
                        "$ref": "#/components/responses/BadRequest"
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "500": {
                        "$ref": "#/components/responses/ServerError"
                    }
                }
            },
            "delete": {
                "description": "Deletes a pool and removes all assigned/unassigned codes and related exports.",
                "summary": "Delete pool",
                "tags": [
                    "Voucher Pool"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/CouponPoolId"
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Success / No Content"
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "409": {
                        "$ref": "#/components/responses/Conflict"
                    },
                    "500": {
                        "$ref": "#/components/responses/ServerError"
                    }
                }
            }
        },
        "/couponPools": {
            "post": {
                "description": "Creates a new coupon pool.",
                "summary": "Create pool",
                "tags": [
                    "Voucher Pool"
                ],
                "requestBody": {
                    "$ref": "#/components/requestBodies/CouponPoolForCreate"
                },
                "responses": {
                    "201": {
                        "description": "Success / Created",
                        "headers": {
                            "Location": {
                                "$ref": "#/components/headers/Location"
                            }
                        },
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/CouponPool"
                                        },
                                        {
                                            "$ref": "#/components/schemas/AllowNewCodeToSameContact"
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "400": {
                        "$ref": "#/components/responses/BadRequest"
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "500": {
                        "$ref": "#/components/responses/ServerError"
                    }
                }
            },
            "get": {
                "description": "Lists the coupon pools.",
                "summary": "List pools",
                "tags": [
                    "Voucher Pool"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/PaginationLimit"
                    },
                    {
                        "$ref": "#/components/parameters/PaginationOffset"
                    }
                ],
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/CouponPools"
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "500": {
                        "$ref": "#/components/responses/ServerError"
                    }
                }
            }
        },
        "/couponPools/{couponPoolId}/codes": {
            "post": {
                "description": "Adds new codes to the pool.",
                "summary": "Adding codes to an existing pool",
                "tags": [
                    "Voucher Codes"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/CouponPoolId"
                    }
                ],
                "requestBody": {
                    "$ref": "#/components/requestBodies/CouponCodes"
                },
                "responses": {
                    "201": {
                        "description": "Ok",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "numberOfLoadedCodes": {
                                            "type": "integer",
                                            "minimum": 0,
                                            "example": 9712
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/Errors"
                                        },
                                        {
                                            "properties": {
                                                "error": {
                                                    "properties": {
                                                        "code": {
                                                            "example": "bad_request"
                                                        },
                                                        "message": {
                                                            "example": "/codes must be an array"
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "500": {
                        "$ref": "#/components/responses/ServerError"
                    }
                }
            }
        },
        "/couponPools/{couponPoolId}/reset": {
            "post": {
                "description": "Resets the pool (deleting all codes from the pool).",
                "summary": "Reset pool",
                "tags": [
                    "Voucher Codes"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/CouponPoolId"
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Success / No Content"
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "500": {
                        "$ref": "#/components/responses/ServerError"
                    }
                }
            }
        },
        "/couponPools/{couponPoolId}/resetContacts": {
            "post": {
                "description": "Resets any codes assigned to the contacts provided from the pool and removes codes assigned earlier so the contacts can have a new one assigned to them.",
                "summary": "Reset codes for contacts",
                "tags": [
                    "Voucher Codes"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/CouponPoolId"
                    }
                ],
                "requestBody": {
                    "$ref": "#/components/requestBodies/ContactIds"
                },
                "responses": {
                    "204": {
                        "description": "Success / No Content"
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/Errors"
                                        },
                                        {
                                            "properties": {
                                                "error": {
                                                    "properties": {
                                                        "code": {
                                                            "example": "bad_request"
                                                        },
                                                        "message": {
                                                            "example": "/contactIds/3 has to be an integer"
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "500": {
                        "$ref": "#/components/responses/ServerError"
                    }
                }
            }
        },
        "/couponPools/{couponPoolId}/exports": {
            "post": {
                "description": "Starts an export for the pool\n- only one export can be running at a time.\n- if the pool has an export already in progress it will return the details of the running export with status code 208.\n",
                "summary": "Start export",
                "tags": [
                    "Export"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/CouponPoolId"
                    }
                ],
                "requestBody": {
                    "$ref": "#/components/requestBodies/Export"
                },
                "responses": {
                    "202": {
                        "$ref": "#/components/responses/RunningExport"
                    },
                    "208": {
                        "$ref": "#/components/responses/RunningExport"
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "500": {
                        "$ref": "#/components/responses/ServerError"
                    }
                }
            }
        },
        "/couponPools/{couponPoolId}/exports/{exportId}": {
            "get": {
                "description": "Returns the details of an export.\n",
                "summary": "Monitor export",
                "tags": [
                    "Export"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/CouponPoolId"
                    },
                    {
                        "$ref": "#/components/parameters/ExportId"
                    }
                ],
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/CompletedExport"
                    },
                    "202": {
                        "$ref": "#/components/responses/RunningExport"
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "500": {
                        "$ref": "#/components/responses/ServerError"
                    }
                }
            }
        },
        "/couponPools/{couponPoolId}/exports/{exportId}/download": {
            "get": {
                "description": "Downloads the exported codes as CSV.",
                "summary": "Download export",
                "tags": [
                    "Export"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/CouponPoolId"
                    },
                    {
                        "$ref": "#/components/parameters/ExportId"
                    }
                ],
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/ExportResult"
                    },
                    "401": {
                        "$ref": "#/components/responses/Unauthorized"
                    },
                    "404": {
                        "$ref": "#/components/responses/NotFound"
                    },
                    "500": {
                        "$ref": "#/components/responses/ServerError"
                    }
                }
            }
        }
    },
    "security": [
        {
            "OAuthAuthorization": []
        }
    ],
    "components": {
        "securitySchemes": {
            "oAuth2ClientCredentials": {
                "type": "oauth2",
                "description": "To use this API, you must first obtain an OAuth 2.0 client credentials token. After that you can use the token to call the API.",
                "flows": {
                    "clientCredentials": {
                        "tokenUrl": "https://{host}/oauth2/token",
                        "scopes": {}
                    }
                }
            }
        },
        "parameters": {
            "CouponPoolId": {
                "name": "couponPoolId",
                "description": "Coupon pool ID",
                "in": "path",
                "required": true,
                "schema": {
                    "type": "integer",
                    "minimum": 0
                }
            },
            "ExportId": {
                "name": "exportId",
                "description": "Coupon pool export ID",
                "in": "path",
                "required": true,
                "schema": {
                    "type": "integer",
                    "minimum": 0
                }
            },
            "PaginationLimit": {
                "description": "Show the first n items",
                "in": "query",
                "name": "$top",
                "schema": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 1000,
                    "default": 100
                }
            },
            "PaginationOffset": {
                "description": "Skip the first n items",
                "in": "query",
                "name": "$skip",
                "schema": {
                    "type": "integer",
                    "minimum": 0,
                    "default": 0
                }
            }
        },
        "headers": {
            "Location": {
                "description": "Path of the created / updated / monitored resource",
                "schema": {
                    "type": "string",
                    "format": "uri"
                }
            },
            "Retry-After": {
                "description": "The number of seconds the client should wait before retrying the request",
                "schema": {
                    "type": "integer",
                    "example": 60
                }
            }
        },
        "requestBodies": {
            "CouponPoolForUpdate": {
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/CouponPool"
                        },
                        "examples": {
                            "one": {
                                "summary": "called with one property",
                                "value": {
                                    "name": "New Name"
                                }
                            },
                            "multiple": {
                                "summary": "called with multiple properties",
                                "value": {
                                    "name": "New Name",
                                    "alertEnabled": true
                                }
                            }
                        }
                    }
                }
            },
            "CouponPoolForCreate": {
                "content": {
                    "application/json": {
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/components/schemas/CouponPool"
                                },
                                {
                                    "$ref": "#/components/schemas/AllowNewCodeToSameContact"
                                }
                            ]
                        },
                        "examples": {
                            "one": {
                                "summary": "called with one property",
                                "value": {
                                    "name": "New Name"
                                }
                            },
                            "multiple": {
                                "summary": "called with multiple properties",
                                "value": {
                                    "name": "New Name",
                                    "alertEnabled": true
                                }
                            }
                        }
                    }
                }
            },
            "CouponCodes": {
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "required": [
                                "codes"
                            ],
                            "properties": {
                                "codes": {
                                    "description": "#### List of codes to add to the pool.\n- if the list contains duplicates, they will be added only once.\n- if the pool already contains the code, it will be ignored.\n",
                                    "type": "array",
                                    "minItems": 1,
                                    "maxItems": 1000000,
                                    "items": {
                                        "type": "string",
                                        "minLength": 4,
                                        "maxLength": 100,
                                        "example": "DijkstraDay7892"
                                    },
                                    "example": [
                                        "ABC123",
                                        "lkjiSDierjknW",
                                        "123456"
                                    ]
                                }
                            }
                        }
                    }
                }
            },
            "ContactIds": {
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "required": [
                                "contactIds"
                            ],
                            "properties": {
                                "contactIds": {
                                    "description": "#### List of contact ids to reset in the pool.\n",
                                    "type": "array",
                                    "minItems": 1,
                                    "maxItems": 1000000,
                                    "items": {
                                        "type": "integer",
                                        "minimum": 0,
                                        "example": 986123
                                    },
                                    "example": [
                                        982381,
                                        901023,
                                        1231
                                    ]
                                }
                            }
                        }
                    }
                }
            },
            "Export": {
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "filters": {
                                    "description": "Optional filters applied to the export.",
                                    "type": "object",
                                    "additionalProperties": false,
                                    "properties": {
                                        "assignedSince": {
                                            "description": "The export will contain only the codes assigned since the given date.",
                                            "type": "string",
                                            "format": "date-time"
                                        }
                                    }
                                }
                            }
                        },
                        "examples": {
                            "withoutFilters": {
                                "summary": "started without filters",
                                "value": {}
                            },
                            "withFilters": {
                                "summary": "started with filters",
                                "value": {
                                    "filters": {
                                        "assignedSince": "2019-01-01T00:00:00Z"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "responses": {
            "CompletedExport": {
                "description": "Completed Coupon Pool Export",
                "content": {
                    "application/json": {
                        "schema": {
                            "anyOf": [
                                {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/FinishedExport"
                                        }
                                    ]
                                },
                                {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/FailedExport"
                                        }
                                    ]
                                }
                            ]
                        }
                    }
                }
            },
            "RunningExport": {
                "description": "Running Coupon Pool Export",
                "headers": {
                    "Location": {
                        "$ref": "#/components/headers/Location"
                    },
                    "RetryAfter": {
                        "$ref": "#/components/headers/Retry-After"
                    }
                },
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "#/components/schemas/RunningExport"
                        }
                    }
                }
            },
            "ExportResult": {
                "description": "Completed Coupon Pool Export",
                "headers": {
                    "Content-Disposition": {
                        "description": "Suggested filename of the export result.",
                        "schema": {
                            "type": "string",
                            "example": "attachment; filename=\"coupon-codes.csv\"; filename*-UTF-8;'coupon-codes.csv"
                        }
                    }
                },
                "content": {
                    "application/csv": {
                        "schema": {
                            "type": "string",
                            "example": "code,contact_id,assigned_at,first_name,last_name,email\nABC123,123,2019-01-01T00:00:00Z,John,Doe,john@do.es\nlkjiSDierjknW,2019-01-01T00:00:00Z,Jane,Doe,jane@do.es\n"
                        }
                    }
                }
            },
            "CouponPools": {
                "description": "Coupon Pools",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "array",
                            "items": {
                                "allOf": [
                                    {
                                        "$ref": "#/components/schemas/CouponPool"
                                    },
                                    {
                                        "$ref": "#/components/schemas/AllowNewCodeToSameContact"
                                    }
                                ]
                            }
                        }
                    }
                }
            },
            "ServerError": {
                "description": "Something went wrong",
                "content": {
                    "application/json": {
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/components/schemas/Errors"
                                },
                                {
                                    "properties": {
                                        "error": {
                                            "properties": {
                                                "code": {
                                                    "example": "internal_server_error"
                                                },
                                                "message": {
                                                    "example": "Something went wrong!"
                                                }
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            },
            "BadRequest": {
                "description": "Bad Request",
                "content": {
                    "application/json": {
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/components/schemas/Errors"
                                },
                                {
                                    "properties": {
                                        "error": {
                                            "properties": {
                                                "code": {
                                                    "example": "bad_request"
                                                },
                                                "message": {
                                                    "example": "/name size must be between 3 and 255"
                                                }
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            },
            "Unauthorized": {
                "description": "Unauthorized",
                "content": {
                    "application/json": {
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/components/schemas/Errors"
                                },
                                {
                                    "properties": {
                                        "error": {
                                            "properties": {
                                                "code": {
                                                    "example": "unauthorized"
                                                },
                                                "message": {
                                                    "example": "Unauthorized!"
                                                }
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            },
            "NotFound": {
                "description": "Resource not found",
                "content": {
                    "application/json": {
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/components/schemas/Errors"
                                },
                                {
                                    "properties": {
                                        "error": {
                                            "properties": {
                                                "code": {
                                                    "example": "not_found"
                                                },
                                                "message": {
                                                    "example": "ResourceName[id=16] not found!"
                                                }
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            },
            "Conflict": {
                "description": "Conflict",
                "content": {
                    "application/json": {
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/components/schemas/Errors"
                                },
                                {
                                    "properties": {
                                        "error": {
                                            "properties": {
                                                "code": {
                                                    "example": "conflict"
                                                },
                                                "message": {
                                                    "example": "The coupon pool is still in use!"
                                                }
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "schemas": {
            "Errors": {
                "type": "object",
                "properties": {
                    "error": {
                        "type": "object",
                        "properties": {
                            "code": {
                                "type": "string"
                            },
                            "message": {
                                "type": "string"
                            }
                        }
                    }
                }
            },
            "Export": {
                "allOf": [
                    {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "id": {
                                "description": "Id of the export.",
                                "type": "integer",
                                "minimum": 1,
                                "readOnly": true,
                                "example": 789
                            },
                            "status": {
                                "description": "Status of the export.",
                                "type": "string",
                                "enum": [
                                    "processing",
                                    "finished",
                                    "failed"
                                ],
                                "default": "processing",
                                "readOnly": true
                            },
                            "downloadUri": {
                                "description": "The download url; set only for `finished` exports",
                                "type": "string",
                                "format": "uri",
